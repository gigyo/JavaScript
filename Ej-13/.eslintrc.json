{

    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": "latest"
    },

    "rules": {
        "indent": ["error", 4],
        "eqeqeq": "warn",
        "strict": "off",
        "quotes": ["error", "double"],
        "semi":["error", "never"]
    }
}